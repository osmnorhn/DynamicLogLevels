{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "LevelSwitches": {
      "$consoleSwitch": "Verbose"
    },
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "levelSwitch": "$consoleSwitch"
        }
      }
    ]
  }
}